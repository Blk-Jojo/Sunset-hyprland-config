{
    "layer": "top", // Waybar at top layer
    "position": "top", // Waybar at the bottom of your screen
    "height": 26, // Waybar height
    // "width": 1366, // Waybar width
    // Choose the order of the modules
    "modules-left": ["custom/power","bluetooth","custom/dunst","clock","tray"],
    "modules-center": ["hyprland/window"],
    "modules-right": ["wireplumber", "network","cpu","memory"],
    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": false,
        "format": "{icon}",
        "format-icons": {
            "1:web": "",
            "2:code": "",
            "3:term": "",
            "4:work": "",
            "5:music": "",
            "6:docs": "",
            "urgent": "",
            "focused": "",
            "default": ""
        }
    },
    "cpu": {
    "format": ""
  },
    "bluetooth": {
    "format": "",
    "tooltip": false,
    "on-click": "blueman-manager"
  },
    "custom/power": {
    "format": "⏻",
    "tooltip": false,
    "on-click": "/home/Ale/.config/waybar/scripts/power_menu.sh"
  },
  "clock": {
        "tooltip-format": "{:%Y-%m-%d | %H:%M}",
        "format-alt": "{:%Y-%m-%d}",
        "format": "{:%H:%M}"
 },
    "tray": {
        // "icon-size": 21,
        "spacing": 10
    },
    "memory": {
        "format": ""
    },
    "network": {
        "on-click": "nm-connection-editor", // (Optional) To force the use of this interface
	"format-wifi": "　{signalStrength}%",
        "format-ethernet": "",
        "format-disconnected": "⚠"
    },
  "custom/dunst": {
  "exec": "dunstctl is-paused | grep -q true && echo '🔕' || echo '🔔'",
  "interval": 2,
  "tooltip": false,
  "on-click": "dunstctl set-paused toggle"  
    },
    "wireplumber": {
        //"scroll-step": 1,
        "format": " {volume}%",
        "format-bluetooth": "{volume}% {icon}",
        "format-muted": "",
        "format-icons": {
            "headphones": "",
            "handsfree": "",
            "headset": "",
            "phone": "",
            "default": ["", ""]
        },
        "on-click": "pavucontrol"
  }
}
  
